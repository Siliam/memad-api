{
  "proto": {
    "id": "?program",
    "title": "$ebucore:title$required$bestlang",
    "keyword": "$ebucore:hasKeyword$var:__keyword",
    "channel": {
      "id": "?__channel_iri",
      "name": "$ebucore:publicationChannelName$required"
    }
  },
  "$where": [
    "?pubhist a ebucore:PublicationHistory",
    "?pubhist ebucore:hasPublicationEvent ebucore:pubevent",
    "?__channel_iri a ebucore:PublicationChannel",
    "?program ebucore:hasPublicationHistory ?pubhist",
    "?pubevent ?ebucore:isReleasedBy ?__channel_iri",
    "?pubevent ebucore:hasPublicationStartDateTime ?date"
  ],
  "$prefixes": {
    "ebucore": "http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#"
  },
  "grlc": {
    "summary": "List of programs.",
    "pagination": 50
  }
}
